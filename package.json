{
  "name": "mobiledgex.js",
  "version": "0.0.0-development",
  "description": "Javascript Client SDK for MobiledgeX",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "nyc check-coverage --statements 80 --branches 70 --functions 80 --lines 80",
    "report-coverage": "nyc report --reporter=lcov && cat ./coverage/lcov.info | codecov",
    "watch:test": "npm t -- -w",
    "test": "mocha src/index.test.js --require @babel/register --timeout 5000",
    "compile": "babel --out-dir dist --source-maps --watch --ignore **/*.test.js src",
    "cover": "nyc npm t",
    "prebuild": "rimraf dist",
    "build:main": "babel --out-dir dist --ignore **/*.test.js src",
    "build:umd": "webpack --mode development --output-filename index.umd.js",
    "build:umd.min": "webpack --mode production --output-filename index.umd.min.js -p",
    "build": "npm-run-all --parallel build:*",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eusholli/mobiledgex.js.git"
  },
  "keywords": [
    "mobiledgex",
    "sdk",
    "js",
    "client"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Geoff Hollingworth <eusholli@gmail.com> (https://linkedin.com/in/geoffhollingworth)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/eusholli/mobiledgex.js/issues"
  },
  "homepage": "https://github.com/eusholli/mobiledgex.js#readme",
  "dependencies": {
    "country-reverse-geocoding": "0.2.2",
    "es6-promise": "4.2.8",
    "isomorphic-fetch": "2.2.1"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.4",
    "@babel/preset-env": "7.5.5",
    "@babel/register": "7.8.6",
    "babel-loader": "8.0.6",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "codecov": "3.7.1",
    "commitizen": "4.0.4",
    "cz-conventional-changelog": "3.1.0",
    "ghooks": "2.0.4",
    "istanbul": "0.4.5",
    "json-loader": "0.5.7",
    "mocha": "7.0.1",
    "npm-run-all": "4.1.5",
    "nyc": "15.0.0",
    "rimraf": "3.0.2",
    "semantic-release": "^17.0.4",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.11"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    }
  }
}
